{
  "name": "phpro/soap-client",
  "description": "A general purpose SoapClient library",
  "keywords": [
    "soap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Toon Verwerft",
      "email": "toon.verwerft@phpro.be"
    }
  ],
  "require": {
    "php": "^8.0",
    "laminas/laminas-code": "^4.4.0",
    "php-soap/engine": "^1.0",
    "php-soap/ext-soap-engine": "^1.0",
    "php-soap/psr18-transport": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "psr/log": "^1.0 || ^2.0 || ^3.0",
    "symfony/console": "~5.3 || ~6.0",
    "symfony/event-dispatcher": "~5.3 || ~6.0",
    "symfony/filesystem": "~5.3 || ~6.0",
    "symfony/validator": "~5.3 || ~6.0"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.4.0",
    "nyholm/psr7": "^1.4",
    "php-http/vcr-plugin": "^1.2",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpro/grumphp-shim": "^1.9",
    "phpspec/phpspec": "~7.2",
    "phpspec/prophecy-phpunit": "^2.0.1",
    "phpstan/phpstan": "^1.2.0",
    "phpunit/phpunit": "~9.5",
    "squizlabs/php_codesniffer": "~3.6"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpro/grumphp-shim": true
    }
  },
  "autoload": {
    "psr-0": {
      "Phpro\\SoapClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "PhproTest\\SoapClient\\": "test/"
    }
  },
  "bin": [
    "bin/soap-client"
  ]
}